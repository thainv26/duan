    function _runTabProduct() {
        $("#tab-pro-hot .title-tab a").click(function(){
            $("#tab-pro-hot .title-tab a").removeClass("current");
            $(this).addClass("current");
      
            var id = $(this).attr("data-id");
            var type = $(this).attr("data-type");
            var limit = $(this).attr("data-limit");
            var holder = $(this).attr("data-holder");
      
            var url = "/ajax/get_json.php?action=product&action_type=product-list&type=&collection="+id+"sort=order&show="+limit;
            getProductList(url,holder,limit);
      
            $(document).ajaxStop(function(){
                var owlProductHot = $("#product-collection.owl-carousel");
                owlProductHot.trigger('destroy.owl.carousel');
      
                
                owlProductHot.owlCarousel({
                    items:4,
                    autoplay:4000,
                    loop:true,
                    slideSpeed : 1000,
                    lazyLoad : true,
                    nav: true,
                    dots:false,
                    responsive : {
                        1200 : {
                            items:5,
                        },
                        1400 : {
                            items:5,
                        },
                        1650 : {
                            items:6,
                        }
                    }
                });
                

            });
        });
    }

    //

    function getBanner(url,location,holder,limit){
        var key = "location_"+location;
        var html = "";
        $.getJSON(url,function(data){
            //console.log("data",data);
            var data = data.list;
            if(data.total > 0){
                data[key].forEach(function(item,index){
                    html+= `
                        <div class="item">
                            <a href="`+ data[key].desUrl +`"><img src="`+ data[key].fileUrl +`" alt="`+ data[key].title +`"></a>
                        </div>
                    `;
                });
                $(holder).html(html);
              
                $(holder +"~ .product-list").addClass("has-banner"); 
            }
        });
    }

    function getProductCategoryChild() {
        $(".product-category-child").each(function(){
            var id = $(this).attr("data-id");
            var holder = $(this).attr("data-holder");
            var limit = $(this).attr("data-limit");
            var holder_banner = $(this).attr("data-holder_banner");
            var url = "/ajax/get_json.php?action=product&action_type=product-list&category="+id+"&type=&sort=order&show="+limit;
          
            getProductList(url,holder,limit);
            getBanner("/ajax/get_json.php?action=banner&action_type=list&template=product_list&location=33&show=1&category="+id,33,holder_banner,1);
        });
    }
</script>


