    $(window).resize(function(){        
        if (window.innerWidth > 1500) {
            setFilterLeftRight(950);
        }  else {
            setFilterLeftRight(650);
        }
    });
  
    window.onload = (event) => {
        setFilterLeftRight(650);
    };
  
    function setFilterLeftRight(maxWidth) {
        let width = $('#js-filter-container').width();

        $(".filter-item").each(function() {
            var checkPosition = width - $(this).offset().left - 150;
                        
            if (checkPosition < maxWidth) {
                $(this).find('.filter-sub').css('right',checkPosition);
            } else if (checkPosition > 1000) {
                $(this).find('.filter-sub').css('left', '0');  
            } else {
                $(this).find('.filter-sub').css('left', $(this).offset().left - 150);
            }
        })
    }

    function showFilterSub() {
        $('.js-filter-title').click(function(){
            var $row = $(this).parents('.filter-item');

            if ($row.hasClass('active') == true) {
                $row.removeClass('active');

                $('.bg-popup').hide();
                $('body').css('overflow','unset');
            }else{
                $('.product-filter-container .filter-item').removeClass('active');
                $row.addClass('active');

                if ($row.hasClass('filter-large-item') == true) {
                    $('body').css('overflow','hidden');
                    $('.bg-popup').show();
                    $("body,html").animate({ scrollTop: $("#js-filter-container").offset().top - 30},800);
                }
            }

        });

        $('body').click(function(e){
            var targ = $(".filter-item");
            if (!targ.is(e.target) && targ.has(e.target).length === 0){                
                $('.filter-item, .filter-sub').removeClass('active');
            }
        });  
    }

    function closePopupLarge() {
        $('.bg-popup').hide();
        $('.filter-item').removeClass('active');
        $('body').css('overflow','unset');
    }
</script>







 	





